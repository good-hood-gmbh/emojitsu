{
  "name": "emojitsu",
  "description": "Converts emojis to images",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": "goodhood-eu/emojitsu",
  "bugs": "https://github.com/goodhood-eu/emojitsu/issues",
  "version": "2.0.0",
  "keywords": [
    "emoji",
    "emojione",
    "emojify"
  ],
  "main": "lib/index.js",
  "engines": {
    "node": "^8.x.x"
  },
  "scripts": {
    "clean": "rm -rf ./lib/ || exit 1",
    "build:babel": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "prepublishOnly": "npm run clean && npm run build:babel",
    "generate": "npm run build:babel && node run.js generate",
    "start": "npm run build:babel && node preview.js",
    "lint": "eslint ./",
    "lint:tofile": "eslint --no-color -o ./eslint-report.log ./",
    "test": "npm run build:babel && env TZ='Europe/Berlin' mocha \"test/**/*.js\"",
    "test:tofile": "npm run build:babel && mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "emojione-assets": "4.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.3",
    "lodash.uniq": "^4.5.0",
    "mocha": "^5.2.0",
    "regexgen": "^1.3.0",
    "request": "^2.88.0"
  },
  "dependencies": {
    "escape-html": "^1.0.3"
  }
}
